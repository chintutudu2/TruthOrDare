{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@api": "./src/Api",
          "@app": "./src/App",
          "@assets": "./src/Assets",
          "@config": "./src/Config",
          "@constants": "./src/Constants",
          "@utils": "./src/Utils",
          "@components": "./src/Components",
          "@screens": "./src/Screens",
          "@interfaces": "./src/Interfaces",
          "@navigators": "./src/Navigators",
          "@helpers": "./src/Helpers"
        }
      }
    ]
  ]
}
